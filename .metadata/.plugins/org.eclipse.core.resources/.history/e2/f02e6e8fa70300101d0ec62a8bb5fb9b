package com.dhinesh.loginService.service;

import org.springframework.stereotype.Service;

@Service
public class OTPService {

	int start = 100000;
	int end = 999999;
	public int generateOTP() {
		double otp = Math.random();
		
		while(otp>start && otp<end) {
			return (int)otp;
		}
		return 0;
	}
}
