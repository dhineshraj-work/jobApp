package com.dhinesh.loginService.controller;

import java.util.Date;

import com.dhinesh.loginService.model.UserModel;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "user_history")
public class UserHistory {

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long uhId;

	private UserModel user;
	
	private String otp;
	private Date lastLoginDate;
	private Date lastPassworDate;
	private Date createDate;
	
	public UserHistory(UserModel user, String otp, Date lastLoginDate, Date lastPassworDate, Date createDate) {
		this.user = user;
		this.otp = otp;
		this.lastLoginDate = lastLoginDate;
		this.lastPassworDate = lastPassworDate;
		this.createDate = createDate;
	}
	public Long getUhId() {
		return uhId;
	}
	public void setUhId(Long uhId) {
		this.uhId = uhId;
	}
	public UserModel getUser() {
		return user;
	}
	public void setUser(UserModel user) {
		this.user = user;
	}
	public String getOtp() {
		return otp;
	}
	public void setOtp(String otp) {
		this.otp = otp;
	}
	public Date getLastLoginDate() {
		return lastLoginDate;
	}
	public void setLastLoginDate(Date lastLoginDate) {
		this.lastLoginDate = lastLoginDate;
	}
	public Date getLastPassworDate() {
		return lastPassworDate;
	}
	public void setLastPassworDate(Date lastPassworDate) {
		this.lastPassworDate = lastPassworDate;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	@Override
	public String toString() {
		return "UserHistory [uhId=" + uhId + ", user=" + user + ", otp=" + otp + ", lastLoginDate=" + lastLoginDate
				+ ", lastPassworDate=" + lastPassworDate + ", createDate=" + createDate + "]";
	}
	
}
